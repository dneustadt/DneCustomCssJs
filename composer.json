{
    "name": "dne/custom-css-js",
    "description": "Custom CSS/JS",
    "version": "3.0.0",
    "type": "shopware-platform-plugin",
    "license": "MIT",
    "require": {
        "shopware/core": "~v6.5.0"
    },
    "authors": [
        {
            "name": "Open Source Plugins"
        }
    ],
    "extra": {
        "shopware-plugin-class": "Dne\\CustomCssJs\\DneCustomCssJs",
        "copyright": "(c) David Neustadt",
        "label": {
            "de-DE": "Custom CSS/JS",
            "en-GB": "Custom CSS/JS"
        },
        "description": {
            "de-DE": "Fügt dem Backend einen Container Manager für JavaScript/SCSS hinzu.",
            "en-GB": "Container manager to easily add custom JavaScript and/or SCSS to the theme compilation."
        },
        "manufacturerLink": {
            "de-DE": "https://store.shopware.com/open-source-plugins.html",
            "en-GB": "https://store.shopware.com/en/open-source-plugins.html"
        },
        "supportLink": {
            "de-DE": "https://github.com/dneustadt",
            "en-GB": "https://github.com/dneustadt"
        }
    },
    "autoload": {
        "psr-4": {
            "Dne\\CustomCssJs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dne\\CustomCssJs\\Test\\": "tests/"
        }
    },
    "suggest": {
        "shopware/storefront": "Require '6.5'.",
        "shopware/administration": "Require '6.5'. Allows you to to set up the plugin easily"
    },
    "conflict": {
        "shopware/storefront": "<6,>=7",
        "shopware/administration": "<6,>=7"
    }
}
